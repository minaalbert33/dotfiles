# open web browser
alt + w
    brave	
#
alt + Return
    ~/.local/scripts/tmux_new_window.sh

alt + s
    ~/.local/scripts/tmux_new_window.sh "find | fzf | xargs xdg-open"

# Open List of Vms installed in ~/vms 
alt + v
	~/.local/scripts/virt.sh	
# Opens clipboard manager alt + c
	clipmenu

# launch file manager menu
alt + f
     ~/.local/scripts/tmux_new_window.sh lfub

# Sytem Monitor
alt + m
    ~/.local/scripts/tmux_new_window.sh btop

# launch gpt 
alt + g
	~/.local/bin/gpt  

# alt + s
# 	~/.local/scripts/screencast.sh	; pkill -RTMIN+10 dwmblocks

# alt + q
#     kill `pidof ffmpeg` ; rm /tmp/record ; pkill -RTMIN+10 dwmblocks
 

# launch powermenu
alt + F4
	~/.local/scripts/powermenu.sh	

# Sends dwmblocks signal to update lang
ctrl + space
    pkill -RTMIN+30 dwmblocks

# Screen Locker
alt + ctrl + Delete
    slock

# Take screenshot
Print
	maim -s  | xclip -selection clipboard -t image/png

# Manipulate Sound
alt + {F1,F2,F3}
    ~/.local/scripts/sound.sh {=,-,+}


# Decrease brightness
alt + F5
	light -U 10 && pkill -RTMIN+25 dwmblocks

# Increase brightness
alt + F6
	light -A 10  && pkill -RTMIN+25 dwmblocks
